// jsDrawChart
function drawChartGeoChartID169bc154ec() {
var data = gvisDataGeoChartID169bc154ec();
var options = {};
options["width"] = 300;
options["height"] = 200;
options["region"] = "US";
options["displayMode"] = "regions";
options["colorAxis"] = {minValue: 0, maxValue: 11, colors:['green', 'red']};
options["resolution"] = "provinces";


    var chart = new google.visualization.GeoChart(
    document.getElementById('GeoChartID169bc154ec')
    );
    chart.draw(data,options);
    

}
  


